class Hero:

    def __init__(self, name, health, power):
        self.__name = name
        self.__health = health
        self.__power = power

    def getName(self):
        return self.__name

    def getHealth(self):
        return self.__health

    def getPower(self):
        return self.__power

    def diserang(self, Power_Serangan):
        self.__health -= Power_Serangan

sniper = Hero("Sniper", 100, 5)

print(sniper.getName())
print(sniper.getHealth())
sniper.diserang(20)
print(sniper.getHealth())
